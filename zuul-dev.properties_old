eureka.client.serviceUrl.defaultZone=http://localhost:8761/eureka/

zuul.prefix=/api
zuul.routes.cep.url=https://viacep.com.br/ws/
zuul.routes.cep.path=/cep/**

zuul.routes.cliente.path=/cliente/**
zuul.routes.cliente.serviceId=cliente
zuul.routes.cliente.stripPrefix=false
# rota -> {url}/api/cliente/1

zuul.routes.acesso.path=/acesso/**
zuul.routes.acesso.serviceId=acesso
zuul.routes.acesso.stripPrefix=false
# rota -> {url}/api/acesso/1

zuul.routes.porta.path=/porta/**
zuul.routes.porta.serviceId=porta
zuul.routes.porta.stripPrefix=false
# rota -> {url}/api/porta/1
